{literal}
<script type="text/x-handlebars-template" id="wallet_bitcoin-core">
	<div class="col-sm-6">
		<div class="card wallet-card">
			<div class="card-header">
				{{name}}
			</div>
			<div class="card-body">
				<h2>Balances</h2>
				<table class="table table-striped table-sm">
					<tr>
						<td>Confirmed</td>
						<td class="number">{{balance}}</td>
					</tr>
					<tr>
						<td>Unconfirmed</td>
						<td class="number">{{unconfirmedBalance}}</td>
					</tr>
					<tr>
						<td>Immature</td>
						<td class="number">{{immatureBalance}}</td>
					</tr>
				</table>
			</div>
		</div>
	</div>
</script>
{/literal}